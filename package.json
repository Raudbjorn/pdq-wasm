{
  "name": "pdq-wasm",
  "version": "0.1.0",
  "description": "WebAssembly bindings for Meta's PDQ perceptual image hashing algorithm",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build:wasm": "bash scripts/build-wasm.sh",
    "build:ts": "tsc && tsc --module es2015 --outDir dist/esm",
    "build": "npm run build:wasm && npm run build:ts",
    "test": "jest",
    "clean": "rm -rf dist build",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "pdq",
    "perceptual-hashing",
    "image-hashing",
    "wasm",
    "webassembly",
    "image-similarity",
    "content-moderation",
    "threat-exchange"
  ],
  "author": "",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/pdq-wasm.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/pdq-wasm/issues"
  },
  "homepage": "https://github.com/yourusername/pdq-wasm#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
